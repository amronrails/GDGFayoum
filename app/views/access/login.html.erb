<h1>login</h1>
	<%= form_tag(:action => 'attempt_login') do %>
	 <div class="field">
    <%= label_tag(:username)+ ":" %>
    <%= text_field_tag(:username)%>
    </div>
    <div class="field">
    <%= label_tag(:password)+ ":" %>
    <%= password_field_tag(:password) %>
    </div>
  <div class="actions">
  <%= submit_tag "Log In", calss: "btn btn-primary" %>
  </div>
  <% end %>


